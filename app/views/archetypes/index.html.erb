<h1>Listing archetypes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archetypes.each do |archetype| %>
      <tr>
        <td>
          <%= archetype.namespace.name + ":" + archetype.name %>
          
        </td>
        <td>
          <% archetype.resources.each do |resource| %>
            <%= link_to resource.relationships.prefLabels_for_this_locale.first.object.literal, '/c/' + resource.literal + "?locale=#{I18n.locale}" %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
